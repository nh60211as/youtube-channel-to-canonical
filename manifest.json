{
  "manifest_version": 2,
  "name": "YouTubeChannelToCanonical",
  "version": "0.0.1",
  "description": "Convert YouTube channel to its canonical form.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/@*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
